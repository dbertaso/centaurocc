	<queryString><![CDATA[select	empresa.nombre as nombre_empresa, 
	empresa.rif,

	solicitud.numero as numero_solicitud,
	partida.nombre as nombre_partida,
	tipo_credito.nombre as nombre_tipo_credito,
	prestamo.tasa_fija,
	prestamo.tasa_vigente,
	prestamo.numero as numero_prestamo,
	prestamo.base_calculo_intereses,
	prestamo.meses_fijos_sin_cambio_tasa,
	prestamo.monto_solicitado as prestamo_monto_solicitado,
	prestamo.dia_facturacion,
	prestamo.fecha_inicio,

	prestamo.cantidad_cuotas_vencidas,
	(prestamo.monto_cuotas_vencidas-plan_pago_cuota.valor_cuota) as monto_vencido,
	plan_pago_cuota.valor_cuota,
	prestamo.monto_mora,
	prestamo.dias_mora,
	prestamo.interes_desembolso_vencido,
	prestamo.interes_diferido_vencido,

	prestamo.dia_facturacion,
	cast(extract(day from plan_pago_cuota.fecha) as integer) as dia_cuota,

	((monto_cuotas_vencidas-valor_cuota)+valor_cuota+monto_mora+
	interes_desembolso_vencido+interes_diferido_vencido) as total_exigible,


	plan_pago_cuota.numero,

	empresa.id as id_empresa,
	solicitud.id as id_solicitud,
	partida.id as id_partida,
	tipo_credito.id as id_tipo_credito,
	prestamo.id as id_prestamo

from	empresa, solicitud, partida, tipo_credito, prestamo, cliente, programa,
	plan_pago, plan_pago_cuota

where	solicitud.cliente_id = cliente.id
and	solicitud.programa_id = programa.id
and	programa.tipo_credito_id = tipo_credito.id
and	cliente.empresa_id = empresa.id
and	prestamo.solicitud_id = solicitud.id
and	plan_pago.prestamo_id = prestamo.id
and	plan_pago_cuota.plan_pago_id = plan_pago.id
and	plan_pago_cuota.fecha >= $P{p_fecha}
and	prestamo.id = $P{p_id_prestamo}
and	plan_pago.activo = 'true'
and	plan_pago.proyeccion = 'false'
order by plan_pago_cuota.fecha asc
limit 1]]></queryString>

