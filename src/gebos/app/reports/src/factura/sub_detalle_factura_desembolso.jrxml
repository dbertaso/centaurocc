	<field name="referencia" class="java.lang.String"/>
	<field name="monto" class="java.lang.Double"/>
	<field name="beneficiario_solicitante" class="java.lang.Boolean"/>
	<field name="beneficiario_tercero" class="java.lang.String"/>
