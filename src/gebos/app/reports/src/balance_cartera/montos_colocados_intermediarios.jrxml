	<queryString><![CDATA[select  entidad_financiera.nombre as nombre_entidad_financiera, 
	prestamo.numero as numero_prestamo, 
	empresa.nombre as nombre_empresa,
	solicitud.fecha_acta_directorio,
	prestamo.monto_liquidado as monto_prestamo,
	prestamo.meses_gracia as plazo_gracia, prestamo.plazo as plazo_amo, 
	prestamo.tasa_fija as regimen,
	prestamo.porcentaje_tasa_foncrei as tasa_foncrei, 
	prestamo.porcentaje_tasa_intermediario as tasa_intermediario,
	prestamo.deuda as saldo_deudor,
	
	entidad_financiera.id as id_entidad_financiera

from prestamo, cliente, empresa, entidad_financiera, solicitud
where prestamo.solicitud_id = solicitud.id
and prestamo.entidad_financiera_id = entidad_financiera.id
and prestamo.cliente_id = cliente.id
and cliente.empresa_id = empresa.id
and solicitud.fecha_acta_directorio 
    between $P{p_fecha_inicio}
    and  $P{p_fecha_fin}

and prestamo.estatus in ('V', 'E')
order by id_entidad_financiera, prestamo.numero]]></queryString>

