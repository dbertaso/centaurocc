	<queryString><![CDATA[select	programa.alias as nombre_programa, 
	partida.nombre as nombre_partida, 
	entidad_financiera.nombre as nombre_entidad_financiera, 
	prestamo.numero, 
	empresa.nombre as nombre_empresa, 
	prestamo.monto_mora, 
	prestamo.interes_vencido + prestamo.causado_no_devengado as interes_vencido, 
	prestamo.interes_diferido_vencido, 
	prestamo.capital_vencido, 
	prestamo.saldo_insoluto - prestamo.capital_vencido as saldo_insoluto,
	prestamo.remanente_por_aplicar, 
	
	programa.id as id_programa, 
	partida.id as id_partida,
	entidad_financiera.id as id_entidad_financiera,
	region.nombre as nombre_region,
	region.id as id_region,

	(prestamo.monto_mora + prestamo.interes_vencido +
	 prestamo.causado_no_devengado + prestamo.interes_diferido_vencido  +
	 prestamo.capital_vencido + (saldo_insoluto - prestamo.capital_vencido) +
	 prestamo.remanente_por_aplicar) as deuda

from prestamo left outer join entidad_financiera on
	prestamo.entidad_financiera_id = entidad_financiera.id, 
	partida, cliente, programa, empresa, solicitud, ciudad,
	estado, region
where	prestamo.solicitud_id = solicitud.id
and 	solicitud.ciudad_id = ciudad.id
and 	ciudad.estado_id = estado.id
and 	estado.region_id = region.id
and 	solicitud.programa_id = programa.id
and 	prestamo.partida_id = partida.id
and 	prestamo.cliente_id = cliente.id
and 	cliente.empresa_id = empresa.id
and 	prestamo.estatus in ('V', 'E', 'R' )
order by id_region, id_partida, numero, id_entidad_financiera]]></queryString>

