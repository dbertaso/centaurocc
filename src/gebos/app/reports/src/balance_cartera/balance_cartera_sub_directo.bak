	<queryString><![CDATA[select programa.alias as nombre_programa, partida.nombre as nombre_partida, 
	prestamo.numero, empresa.nombre as nombre_empresa, prestamo.monto_mora, 
	prestamo.interes_vencido + prestamo.causado_no_devengado as interes_vencido,  
	prestamo.interes_diferido_vencido, prestamo.capital_vencido, prestamo.saldo_insoluto,
	prestamo.remanente_por_aplicar, prestamo.deuda, 

	programa.id as id_programa, 
	partida.id as id_partida
from prestamo, partida, cliente, programa, empresa, solicitud
where prestamo.solicitud_id = solicitud.id
and solicitud.programa_id = programa.id
and prestamo.partida_id = partida.id
and prestamo.cliente_id = cliente.id
and cliente.empresa_id = empresa.id
and prestamo.estatus in ('V', 'E', 'R' )
--and dias_mora > 0
and prestamo.intermediado = false
order by id_programa, id_partida, prestamo.numero]]></queryString>

