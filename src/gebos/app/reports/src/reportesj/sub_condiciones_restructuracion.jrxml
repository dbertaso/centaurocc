	<queryString><![CDATA[select	partida.nombre as nombre_partida, 
	prestamo_modificacion.monto,
	prestamo_modificacion.plazo, 
	prestamo_modificacion.tasa_inicial as tasa_prestamo_inicial, 
	prestamo_modificacion.meses_muertos, 
	prestamo_modificacion.frecuencia_pago,
	prestamo_modificacion.plazo / prestamo_modificacion.frecuencia_pago as cantidad_cuotas,
	prestamo_modificacion.meses_fijos_sin_cambio_tasa,
	programa.porcentaje_tasa_intermediario

from	prestamo, partida, solicitud, programa, prestamo_modificacion

where	prestamo.partida_id = partida.id
and	prestamo.solicitud_id = solicitud.id
and	solicitud.programa_id = programa.id
and	prestamo_modificacion.prestamo_id = prestamo.id
and	prestamo_modificacion.estatus in ('E')
and	prestamo_modificacion.tipo in ('F')
and	solicitud.id = $P{p_id_solicitud}]]></queryString>

