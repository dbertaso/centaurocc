	<parameter name="PFE_DESDE" isForPrompting="true" class="java.util.Date"/>
	<parameter name="PFE_HASTA" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[SELECT
     estado."nombre" AS estado_nombre,
     region."nombre" AS region_nombre,
     empresa."nombre" AS empresa_nombre,
     empresa."rif" AS empresa_rif,
     solicitud."monto_solicitado" AS solicitud_monto_solicitado,
     usuario."primer_nombre"||' '||usuario."primer_apellido" AS nombres,
     solicitud."numero" AS solicitud_numero,
     CASE WHEN solicitud."estatus"='T'THEN 'Tramitando'WHEN solicitud."estatus"='I'THEN 'En Directorio'WHEN solicitud."estatus"='A'THEN 'Aprobada'WHEN solicitud."estatus"='C'THEN 'Condicionada Aprobada'WHEN solicitud."estatus"='F'THEN 'Diferido'WHEN solicitud."estatus"='R'THEN 'Rechazada'WHEN solicitud."estatus"='D'THEN 'En DocumentaciÃ³n'WHEN solicitud."estatus"='O'THEN 'Documentada'WHEN solicitud."estatus"='S'THEN 'Condicionando Desembolso'WHEN solicitud."estatus"='K'THEN 'Desembolso Condicionado'WHEN solicitud."estatus"='P'THEN 'Solcitud de Pago'WHEN solicitud."estatus"='L'THEN 'Liquidado Parcial'WHEN solicitud."estatus"='Q'THEN 'Liquidado'ELSE 'Estatus no definido'END AS solicitud_estatus,
     programa."nombre" AS programa_nombre,
     solicitud."fecha_solicitud" AS fecha_solicitud
FROM
     "public"."usuario" usuario INNER JOIN "public"."solicitud_evento" solicitud_evento ON usuario."id" = solicitud_evento."usuario_id"
     INNER JOIN "public"."solicitud" solicitud ON solicitud_evento."solicitud_id" = solicitud."id"
     INNER JOIN "public"."programa" programa ON solicitud."programa_id" = programa."id"
     INNER JOIN "public"."ciudad" ciudad ON solicitud."ciudad_id" = ciudad."id"
     INNER JOIN "public"."cliente" cliente ON solicitud."cliente_id" = cliente."id"
     INNER JOIN "public"."empresa" empresa ON cliente."empresa_id" = empresa."id"
     INNER JOIN "public"."estado" estado ON ciudad."estado_id" = estado."id"
     INNER JOIN "public"."region" region ON estado."region_id" = region."id"
     INNER JOIN "public"."pais" pais ON estado."pais_id" = pais."id"
     AND pais."id" = region."pais_id"
WHERE
     to_date(fecha_solicitud,'YYYYMMDD') >= to_date($P{PFE_DESDE},'YYYYMMDD')
 AND to_date(fecha_solicitud,'YYYYMMDD') <= to_date($P{PFE_HASTA},'YYYYMMDD')
ORDER BY
     region_nombre ASC,
     estado_nombre ASC,
     programa_nombre ASC,
     empresa_nombre ASC]]></queryString>

