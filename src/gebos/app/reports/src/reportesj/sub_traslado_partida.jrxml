	<queryString><![CDATA[select	prestamo_origen.numero as numero_prestamo_origen,
	prestamo_destino.numero as numero_prestamo_destino,
	partida_origen.nombre as nombre_partida_origen,
	partida_destino.nombre as nombre_partida_destino,
	prestamo_modificacion.traslado_monto as monto,
	
	solicitud.id as id_solicitud,
	prestamo_modificacion.id as id_modificacion

from	solicitud, prestamo as prestamo_origen, prestamo as prestamo_destino, 
	partida as partida_origen, partida as partida_destino, prestamo_modificacion

where	prestamo_origen.partida_id = partida_origen.id
and	prestamo_destino.partida_id = partida_destino.id
and	prestamo_modificacion.traslado_prestamo_origen_id = prestamo_origen.id
and	prestamo_modificacion.traslado_prestamo_destino_id = prestamo_destino.id
and	prestamo_modificacion.solicitud_id = solicitud.id
and	prestamo_modificacion.estatus in ('E')
and	prestamo_modificacion.tipo in ('P')
and	solicitud.id = $P{p_id_solicitud}]]></queryString>

