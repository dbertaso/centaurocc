	<parameter name="SUBREPORT_DIR" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["/opt/reportesgbos/prestamos/"]]></defaultValueExpression>
	</parameter>
	<parameter name="p_id_solicitud" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(1)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT
     modalidad_financiamiento."nombre" AS modalidad_financiamiento_nombre,
     region."nombre" AS region_nombre,
     sector_industrial."nombre" AS sector_industrial_nombre,
     programa."nombre" AS programa_nombre,
     case when empresa."nombre" is null then persona.primer_nombre || ' ' || persona.primer_apellido else empresa.nombre end AS empresa_nombre,
     origen_fondo."nombre" AS origen_fondo_nombre,
     tipo_credito."nombre" AS tipo_credito_nombre,
     case when empresa."rif" is null then cast(persona.cedula as varchar) else empresa.rif end AS empresa_rif,
     case when empresa.rif is null then
					persona_direccion.avenida||' '||persona_direccion.edificio||' '||persona_direccion.zona||' '||persona_direccion.referencia
				   else
					empresa_direccion.avenida||' '||empresa_direccion.edificio||' '||empresa_direccion.zona||' '||empresa_direccion.referencia 
     end AS direccion,
     case when empresa.rif is null then 
					persona_telefono.codigo||' '||cast(persona_telefono.numero as varchar)
				   else
					empresa_telefono.codigo||' '||cast(empresa_telefono.numero as varchar) 
     end as empresa_telefono_numero,
     case when empresa.rif is null then persona_email.email else empresa_email."email" end as empresa_email,
     solicitud."monto_solicitado" AS solicitud_monto_solicitado,
     solicitud."monto_aprobado" AS solicitud_monto_aprobado,
     solicitud."fecha_solicitud" AS solicitud_fecha_solicitud,
     solicitud."numero" AS solicitud_numero,
     solicitud."fecha_acta_comite" AS solicitud_fecha_acta_comite,
     solicitud."numero_acta_comite" AS solicitud_numero_acta_comite,
     solicitud."numero_acta_directorio" AS solicitud_numero_acta_directorio,
     solicitud."fecha_acta_directorio" AS solicitud_fecha_acta_directorio,
     solicitud."numero_resolucion_comite" AS solicitud_numero_resolucion_comite,
     solicitud."numero_resolucion_directorio" AS solicitud_numero_resolucion_directorio,
     solicitud."monto_cliente" AS solicitud_monto_cliente,
     solicitud."id" as solicitud_id,
     solicitud."municipio_id" AS solicitud_municipio_id,
     solicitud."parroquia_id" AS solicitud_parroquia_id,
     parroquia."nombre" AS parroquia_nombre,
     municipio."nombre" AS municipio_nombre,
     solicitud."ciudad_id" AS solicitud_ciudad_id,
     solicitud."modalidad_financiamiento_id" AS solicitud_modalidad_financiamiento_id,
     empresa."cantidad_empleados_femeninos" AS empresa_cantidad_empleados_femeninos,
     empresa."cantidad_empleados_masculinos" AS empresa_cantidad_empleados_masculinos,
     empresa."cantidad_empleados_femeninos" + empresa."cantidad_empleados_masculinos" as totalempleados,
     empresa."producto" AS empresa_producto,
     ciudad."nombre" AS ciudad_nombre,
     estado."nombre" AS estado_nombre,
     solicitud."programa_id" AS solicitud_programa_id,
     empresa."id" AS empresa_id,
     solicitud."tir" AS solicitud_tir,
     solicitud."van" AS solicitud_van,
     solicitud."tiempo_recuperacion" AS tiempo_recuperacion
FROM
     "solicitud" solicitud INNER JOIN "cliente" cliente ON solicitud."cliente_id" = cliente."id"
     left JOIN "empresa" empresa ON cliente."empresa_id" = empresa."id"
     left JOIN "empresa_direccion" empresa_direccion ON (empresa."id" = empresa_direccion."empresa_id" and
							  empresa_direccion.tipo = 'P')
     left JOIN "persona" persona ON cliente."persona_id" = persona."id"
     left JOIN "persona_direccion" persona_direccion ON (persona."id" = persona_direccion."persona_id" and
							  persona_direccion.tipo = 'H')
     LEFT JOIN "sector_industrial" sector_industrial ON empresa."sector_industrial_id" = sector_industrial."id"
     LEFT JOIN  empresa_telefono ON ( empresa."id" = empresa_telefono."empresa_id" and
				      empresa_telefono."tipo" = 'P')
     LEFT JOIN empresa_email on (empresa_email.empresa_id = empresa."id")

     LEFT JOIN persona_telefono ON ( persona."id" = persona_telefono."persona_id" and
				      persona_telefono."tipo" = 'H')
     LEFT JOIN persona_email on (persona_email.persona_id = persona."id")
     INNER JOIN "ciudad" ciudad ON solicitud."ciudad_id" = ciudad."id"
     INNER JOIN "modalidad_financiamiento" modalidad_financiamiento ON solicitud."modalidad_financiamiento_id" = modalidad_financiamiento."id"
     INNER JOIN "programa" programa ON solicitud."programa_id" = programa."id"
     INNER JOIN "origen_fondo" origen_fondo ON solicitud."origen_fondo_id" = origen_fondo."id"
     INNER JOIN "public"."parroquia" parroquia ON solicitud."parroquia_id" = parroquia."id"
     INNER JOIN "public"."municipio" municipio ON solicitud."municipio_id" = municipio."id"
     INNER JOIN "tipo_credito" tipo_credito ON programa."tipo_credito_id" = tipo_credito."id"
     INNER JOIN "estado" estado ON ciudad."estado_id" = estado."id"
     INNER JOIN "region" region ON estado."region_id" = region."id"
WHERE
     solicitud."id" = $P{p_id_solicitud}]]></queryString>

