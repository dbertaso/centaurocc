	<variable name="gBeneficiarios" class="java.lang.Integer" resetType="Group" resetGroup="sub_rubro" calculation="Count">
		<variableExpression><![CDATA[$V{sub_rubro_COUNT} + $F{cantidad_integrantes}]]></variableExpression>
