	<parameter name="p_prestamo" class="java.lang.Long">
		<defaultValueExpression><![CDATA[new Long(1)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT DISTINCT
     rubro."nombre" AS partida_nombre,
     programa."nombre" AS programa_nombre,
     case when empresa."nombre"  is null then persona.primer_nombre || ' ' || persona.primer_apellido else empresa.nombre end AS empresa_nombre,
     case when empresa."rif"  is null then cast(persona.cedula as varchar) else empresa.rif end AS empresa_rif,
     prestamo."numero" AS prestamo_numero,
     prestamo."tasa_vigente" AS prestamo_tasa_vigente,
     prestamo."dias_mora" AS prestamo_dias_mora,
     prestamo."monto_mora" AS prestamo_monto_mora,
     prestamo."interes_vencido" AS prestamo_interes_vencido,
     prestamo."saldo_insoluto" AS prestamo_saldo_insoluto,
     prestamo."saldo_insoluto" - prestamo."capital_pago_parcial" as saldo_deudor,
     prestamo."causado_no_devengado" AS prestamo_causado_no_devengado,
     prestamo."interes_diferido_vencido" AS prestamo_interes_diferido_vencido,
     prestamo."remanente_por_aplicar" AS prestamo_remanente_por_aplicar,
     prestamo."cantidad_cuotas_vencidas" AS prestamo_cantidad_cuotas_vencidas,
     prestamo."interes_desembolso_vencido" AS prestamo_interes_desembolso_vencido,
     prestamo."capital_pago_parcial" AS prestamo_capital_pago_parcial,
     prestamo."interes_diferido_por_vencer" AS prestamo_interes_diferido_por_vencer,
     prestamo."deuda" AS prestamo_deuda,
     prestamo."exigible" AS prestamo_exigible,
     prestamo."capital_vencido" AS prestamo_capital_vencido,
     case when empresa.rif is null then
					persona_direccion.avenida||' '||persona_direccion.edificio||' '||persona_direccion.zona||' '||persona_direccion.referencia
				   else
					empresa_direccion.avenida||' '||empresa_direccion.edificio||' '||empresa_direccion.zona||' '||empresa_direccion.referencia
     end AS direccion,
     case when empresa.rif is null then
					persona_telefono.codigo||' '||cast(persona_telefono.numero as varchar)
				   else
					empresa_telefono.codigo||' '||cast(empresa_telefono.numero as varchar)
     end as telefono,
     case when solicitud.por_inventario = true then prestamo.monto_inventario  + prestamo.monto_sras_total  + prestamo.monto_gasto_total else prestamo.monto_insumos + prestamo.monto_banco + prestamo.monto_sras_total  + prestamo.monto_gasto_total end AS prestamo_monto_aprobado,
     prestamo."monto_solicitado" AS prestamo_monto_solicitado,
     case when programa."porcentaje_tasa_foncrei" = 0 then 0.0 else prestamo."tasa_vigente" * programa."porcentaje_tasa_foncrei" / 100 end as tasa_foncrei,
     case when programa."porcentaje_tasa_foncrei" = 0 then 0.0 else prestamo."tasa_vigente" - (prestamo."tasa_vigente" * programa."porcentaje_tasa_foncrei" / 100) end as tasa_intermediario,
     case when programa."porcentaje_tasa_foncrei" != 0 then cast(prestamo."interes_vencido" * (programa."porcentaje_tasa_foncrei" / 100) as decimal(16,2)) else 0.00 end as interes_foncrei,
     case when programa."porcentaje_tasa_foncrei" != 0 then (prestamo."interes_vencido" - cast(prestamo."interes_vencido" * (programa."porcentaje_tasa_foncrei" / 100) as decimal(16,2))) else 0.00 end as interes_intermediario,
     prestamo."tasa_vigente" as tasa_vigente,
     case prestamo."estatus"
      when 'S' then 'En Solicitud'
      when 'D' then 'En espera por Desembolsos'
      when 'H' then 'Vigente - Demorado'
      when 'V' then 'Vigente'
      when 'E' then 'Vencido'
      when 'F' then 'Cancelado / Reestructuración'
      when 'A' then 'Cancelado / Reestructuración Adm'
      when 'L' then 'Litigio'
      when 'C' then 'Cancelado'
      when 'P' then 'Vigente'
      when 'H' then 'Vigente - Demorado'
     end as estatus,
     dias_demorado,
     dias_vencido,
     dias_vigente,
     cuotas_pagadas,
     cuotas_pendientes,
     cuotas_especiales_pagadas,
     cuotas_especiales_vencidas,
     capital_por_pagar,
     prestamo.capital_pagado,
     prestamo.intereses_pagados,
     prestamo.mora_pagada,
     prestamo.codigo_contable as codigo_contable
FROM
     "public"."cliente" cliente INNER JOIN "public"."prestamo" prestamo ON cliente."id" = prestamo."cliente_id"
     left JOIN "public"."empresa" empresa ON cliente."empresa_id" = empresa."id"
     left JOIN "public"."persona" persona ON cliente."persona_id" = persona."id"
     INNER JOIN "public"."solicitud" solicitud ON solicitud."id" = prestamo."solicitud_id"
     INNER JOIN "public"."programa" programa ON solicitud."programa_id" = programa."id"
     INNER JOIN "public"."rubro" rubro ON prestamo."rubro_id" = rubro."id"
     left JOIN "public"."empresa_direccion" empresa_direccion ON (empresa."id" = empresa_direccion."empresa_id" and empresa_direccion.tipo = 'P')
     left JOIN "public"."empresa_telefono" empresa_telefono ON (empresa."id" = empresa_telefono."empresa_id" and empresa_telefono.tipo = 'P')
     left JOIN "public"."persona_direccion" persona_direccion ON (persona."id" = persona_direccion."persona_id" and persona_direccion.tipo = 'H')
     left JOIN "public"."persona_telefono" persona_telefono ON (persona."id" = persona_telefono."persona_id" and persona_telefono.tipo = 'H')
WHERE
     prestamo."numero" = $P{p_prestamo}
limit 1]]>
	</queryString>
