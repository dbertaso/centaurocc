	<parameter name="p_id_solicitud" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(1)]]></defaultValueExpression>
	</parameter>
	<parameter name="SUBREPORT_DIR" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["/opt/reportesgbos/solicitudes/"]]></defaultValueExpression>
	</parameter>
	<parameter name="p_id_prestamo" isForPrompting="false" class="java.lang.Long">
		<defaultValueExpression ><![CDATA[new Long(1)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select	solicitud.id as id_solicitud,
	case when empresa.nombre is null then persona.primer_apellido || ' ' || persona.primer_nombre else empresa.nombre end as nombre_empresa,
	solicitud.numero as numero_solicitud,
	solicitud.fecha_solicitud as fecha_solicitud, 
	case when estado.nombre is null then estado_nat.nombre else estado.nombre end as nombre_estado,
	case when ciudad.nombre is null then ciudad_nat.nombre else ciudad.nombre end as nombre_ciudad, 
	case when municipio.nombre is null then municipio_nat.nombre else municipio.nombre end as nombre_municipio, 
	case when solicitud.fecha_acta_directorio isnull then solicitud.fecha_acta_comite else solicitud.fecha_acta_directorio end as fecha_modificacion, 
	solicitud.monto_solicitado,
	solicitud.monto_cliente,
	programa.nombre as nombre_programa, 
	case when solicitud.numero_acta_directorio isnull then solicitud.numero_acta_comite else solicitud.numero_acta_directorio end as numero_acta,
	solicitud.numero_acta_comite,
	solicitud.fecha_acta_comite,
	solicitud.numero_resolucion_comite,
	case when solicitud.numero_resolucion_directorio isnull then solicitud.numero_resolucion_comite else solicitud.numero_resolucion_directorio end as numero_resolucion,
	solicitud.intermediado, 
	modalidad_financiamiento.nombre as nombre_modalidad_financiamiento,
	solicitud.liberada as liberada,
	solicitud.causa_liberacion as causa_liberacion,

	solicitud.propuesta_social_trabajadores,
	solicitud.propuesta_social_comunidad,
	solicitud.propuesta_social_ambiente,

	solicitud.aporte_social,
	solicitud.monto_propuesta_social,
	solicitud.propuesta_utilidad_neta,
	solicitud.id as id_solicitud,
	case solicitud.tipo_comite when 'C' then 'COMITE CENTRAL' else 'COMITE REGIONAL NO CONCURRENTE' END AS TIPO_COMITE,
	case when sector_industrial.nombre is null then ' ' else sector_industrial.nombre end as nombre

	from	
		solicitud inner join cliente on (solicitud.cliente_id = cliente.id)
                          left join empresa on (cliente.empresa_id = empresa.id)
                          left join persona on (cliente.persona_id = persona.id)
                          left join empresa_direccion on (empresa_direccion.empresa_id = empresa.id and
                                                           empresa_direccion.tipo = 'P')
                          left join persona_direccion on (persona_direccion.persona_id = persona.id and
                                                           persona_direccion.tipo = 'H')
			  left join sector_industrial on (empresa.sector_industrial_id = sector_industrial.id)
                          left join ciudad on (empresa_direccion.ciudad_id = ciudad.id) 
                          left join ciudad ciudad_nat on (persona_direccion.ciudad_id = ciudad_nat.id) 
			  left join estado on (ciudad.estado_id = estado.id)
			  left join estado estado_nat on (ciudad_nat.estado_id = estado_nat.id)
                          left join municipio on (empresa_direccion.municipio_id = municipio.id)
                          left join municipio municipio_nat on (persona_direccion.municipio_id = municipio_nat.id)
			  inner join modalidad_financiamiento on (solicitud.modalidad_financiamiento_id = modalidad_financiamiento.id)
			  inner join programa on (solicitud.programa_id = programa.id)
	
where	
	solicitud."id" = $P{p_id_solicitud}]]></queryString>

