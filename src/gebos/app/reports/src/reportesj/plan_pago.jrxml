	<queryString>
		<![CDATA[SELECT
     case when empresa."nombre" is null then persona.primer_nombre || ' ' || persona.primer_apellido else empresa.nombre end AS empresa_nombre,
     case when empresa."rif" is null then cast(persona.cedula as varchar) else empresa.rif end AS empresa_rif,
     prestamo.codigo_contable as auxiliar_contable,
     rubro."nombre" AS partida_nombre,
     prestamo."monto_liquidado" AS prestamo_monto_aprobado,
     prestamo."numero" AS prestamo_numero,
     prestamo."tasa_vigente" AS prestamo_tasa_vigente,
     prestamo.id as prestamo_id,
     CASE WHEN plan_pago_cuota."tipo_cuota"='C'THEN 'Periodo de Amortización'WHEN plan_pago_cuota."tipo_cuota"='G'THEN 'Periodo de Gracia y/o Diferido'ELSE 'Periodo Muerto'END AS tipo_cuota,
     plan_pago_cuota."numero" AS plan_pago_cuota_numero,
     plan_pago_cuota."fecha" AS plan_pago_cuota_fecha,
     plan_pago_cuota."valor_cuota" AS plan_pago_cuota_valor_cuota,
     plan_pago_cuota."amortizado" AS plan_pago_cuota_amortizado,
     plan_pago_cuota."interes_corriente" AS plan_pago_cuota_interes_corriente,
     solicitud."numero" AS solicitud_numero,
     programa."nombre" AS programa_nombre,
     prestamo."remanente_por_aplicar" AS prestamo_remanente_por_aplicar,
     prestamo."capital_pago_parcial" AS prestamo_capital_pago_parcial,
     prestamo."deuda" AS prestamo_deuda,
     prestamo."exigible" AS prestamo_exigible,
     plan_pago_cuota."pago_interes_mora" AS plan_pago_cuota_pago_interes_mora,
     plan_pago_cuota."pago_interes_corriente" AS plan_pago_cuota_pago_interes_corriente,
     plan_pago_cuota."pago_interes_diferido" AS plan_pago_cuota_pago_interes_diferido,
     plan_pago_cuota."pago_capital" AS plan_pago_cuota_pago_capital,
     plan_pago_cuota."interes_diferido" AS plan_pago_cuota_interes_diferido,
     plan_pago_cuota."interes_mora" AS plan_pago_cuota_interes_mora,
     prestamo."saldo_insoluto" AS prestamo_saldo_insoluto,
     plan_pago_cuota."saldo_insoluto" AS plan_pago_cuota_saldo_insoluto,
     case prestamo."estatus" WHEN 'L'THEN 'LITIGIO'WHEN 'J'THEN 'CONSULTORIA JURIDICA'WHEN 'V'THEN 'VIGENTE'WHEN 'H'THEN 'VIGENTE' WHEN 'E' THEN 'VENCIDO' END AS estatus_prestamo,
     case plan_pago_cuota."estatus_pago" WHEN 'N'THEN 'EXIGIBLE'WHEN 'P'THEN 'PAGO PARCIAL'WHEN 'T'THEN 'TOTALMENTE PAGADA'WHEN 'X'THEN 'NO EXIGIBLE' WHEN 'D' THEN 'DIFERIDA' END AS estatus,
     plan_pago_cuota."dias_mora" AS plan_pago_cuota_dias_mora,
     case prestamo."frecuencia_pago" when 0 then 'Pago Único' when 1 then 'Mensual' when 2 then 'Bimestral' when 3 then 'Trimestral' when 5 then 'Pentamestral' when 6 then 'Semestral' when 12 then 'Anual' END AS prestamo_frecuencia_pago,
     case when programa."porcentaje_tasa_foncrei" = 0 then 0.0 else prestamo."tasa_vigente" * programa."porcentaje_tasa_foncrei" / 100 end as tasa_foncrei,
     case when programa."porcentaje_tasa_foncrei" = 0 then 0.0 else prestamo."tasa_vigente" -  prestamo."tasa_vigente" * programa."porcentaje_tasa_foncrei" / 100 end as tasa_intermediario,
     plan_pago_cuota.valor_cuota + plan_pago_cuota.interes_diferido as pago_total_cliente

FROM
     "public"."cliente" cliente INNER JOIN "public"."prestamo" prestamo ON cliente."id" = prestamo."cliente_id"
     LEFT JOIN "empresa" empresa ON cliente."empresa_id" = empresa."id"
     LEFT JOIN "persona" persona ON cliente."persona_id" = persona."id"
     INNER JOIN "public"."rubro" rubro ON prestamo."rubro_id" = rubro."id"
     INNER JOIN "public"."plan_pago" plan_pago ON prestamo."id" = plan_pago."prestamo_id"
     INNER JOIN "public"."solicitud" solicitud ON prestamo."solicitud_id" = solicitud."id"
     INNER JOIN "public"."programa" programa ON solicitud."programa_id" = programa."id"
     INNER JOIN "public"."plan_pago_cuota" plan_pago_cuota ON plan_pago."id" = plan_pago_cuota."plan_pago_id"
WHERE
     plan_pago."activo" = 'true'
     and prestamo."numero" = $P{p_prestamo}
ORDER BY
     tipo_cuota DESC,
     plan_pago_cuota."numero" ASC]]>
