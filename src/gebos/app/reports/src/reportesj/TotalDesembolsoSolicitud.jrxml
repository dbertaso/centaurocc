	<parameter name="p_solicitud_id" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(1)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT
     partida."nombre" AS partida_nombre,
     prestamo."id" AS ID,
     prestamo."numero",
     prestamo."monto_solicitado" AS prestamo_monto_aprobado,
     (prestamo."monto_solicitado"-desembolso."monto")as monto_por_desembolsar,
     desembolso."monto" AS desembolso_monto,
     solicitud."id" AS solicitud_id
FROM
     "public"."prestamo" prestamo 
     				INNER JOIN "public"."partida" partida ON prestamo."partida_id" = partida."id"
     				INNER JOIN "public"."desembolso" desembolso ON (prestamo."id" = desembolso."prestamo_id" and
									       desembolso.realizado = true)
     				INNER JOIN "public"."solicitud" solicitud ON (prestamo."solicitud_id" = solicitud."id" and
									     solicitud.id = $P{p_solicitud_id})
WHERE
     (prestamo."estatus" = 'V'
     or prestamo."estatus" = 'P'
     or prestamo."estatus" = 'E')
ORDER BY
     prestamo."partida_id" ASC,
     prestamo."id" ASC]]></queryString>

