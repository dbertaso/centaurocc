	<parameter name="p_id_factura" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select

  case when empresa.rif is null then persona.primer_apellido || ' ' || persona.primer_nombre else empresa.nombre end as nombre_empresa,
	case when empresa.rif is null then 'Cédula: ' || cast(persona.cedula as varchar) else 'RIF: ' || empresa.rif end as rif,
  case when sen.descripcion is null then sej.descripcion else sen.descripcion end as sector_economico,
	solicitud.numero as numero_solicitud,
	solicitud.monto_solicitado,
	programa.nombre as nombre_programa,
	rubro.nombre as nombre_partida,
	prestamo.tasa_fija,
	prestamo.tasa_vigente,
	prestamo.numero as numero_prestamo,
	prestamo.monto_solicitado as prestamo_monto_solicitado,
	prestamo.meses_fijos_sin_cambio_tasa,
	prestamo.frecuencia_pago,
	prestamo.fecha_inicio,
	prestamo.base_calculo_intereses,
	tipo_credito.nombre as nombre_tipo_credito,
	prestamo.dia_facturacion,
        prestamo.codigo_contable as codigo_contable,
	factura.numero as numero_factura,

	factura.tipo as tipo_factura,
	factura.proceso_nocturno as proceso_nocturno,
	factura.fecha as fecha_facturacion,
	factura.fecha_realizacion as fecha_realizacion,
	factura.monto as monto,
	factura.remanente_por_aplicar as saldo_favor,
          factura.pago_interes as pago_interes,
	factura.comentario_analista,
        factura.recuperaciones as recuperaciones,
        case when factura.distincion_cobranza = 'REZ' then 'Rezagada' else 'Del mes' end as distinción,
        factura.analista,
	pago_cliente.modalidad as modalidad,
        factura.consultoria_juridica,
        factura.estado,
        factura.sector_economico,
	rubro.id as id_partida,
	programa.id as id_programa,
	prestamo.id as id_prestamo,
	solicitud.id as id_solicitud,
	factura.id as id_factura,
	case when empresa.id is null then persona.id else empresa.id end as id_empresa,
	programa.alias as programa_alias,
	origen_fondo.descripcion as origen_fondo_descripcion,
	pago_cliente.id as id_pago_cliente,
        tipo_cartera.nombre as tipo_cartera,
        factura.estado_geografico,
        tipo_cartera.nombre as nombre_cartera,
        prestamo.monto_liquidado,
        pago_cliente.efectivo as monto_efectivo,
        factura.abono_capital,
        prestamo.codigo_contable,
        pago_cliente.numero_voucher,
        entidad_financiera.nombre as banco_recaudador,
        pago_cliente.id,
        prestamo.codigo_contable,
        cliente.id as cliente_id

from	solicitud
        inner join prestamo on (prestamo.solicitud_id = solicitud.id)
        inner join cliente on (solicitud.cliente_id = cliente.id)
        inner join rubro on (prestamo.rubro_id = rubro.id)
        left join tipo_cartera on (prestamo.tipo_cartera_id = tipo_cartera.id)
        left join empresa on ( cliente.empresa_id = empresa.id)
        left join persona on (cliente.persona_id = persona.id)
        left join sector_economico sej on (empresa.sector_economico_id = sej.id)
        left join sector_economico sen on (persona.sector_economico_id = sen.id),
        tipo_credito, programa,
	factura, pago_cliente,origen_fondo,entidad_financiera


where prestamo.tipo_credito_id = tipo_credito.id
and	prestamo.solicitud_id = solicitud.id
and	prestamo.rubro_id = rubro.id
and	solicitud.programa_id = programa.id
and	factura.pago_cliente_id = pago_cliente.id
and	factura.prestamo_id = prestamo.id
and	solicitud.origen_fondo_id = origen_fondo.id
and     pago_cliente.entidad_financiera_id = entidad_financiera.id
and	factura.id = $P{p_id_factura}
and     (factura.tipo = 'P' or factura.tipo = 'R' or factura.tipo = 'M')
order by id_solicitud, id_partida, id_factura]]>
