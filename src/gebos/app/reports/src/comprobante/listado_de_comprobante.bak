	<queryString><![CDATA[select	comprobante_contable.id as numero_comprobante,
	comprobante_contable.fecha_comprobante,
	comprobante_contable.fecha_registro,
	prestamo.numero as numero_contrato,
	cuenta_contable.nombre as cuenta_contable,
	cuenta_contable_presupuesto.codigo as cuenta_presupuesto,
	asiento_contable.monto,
	asiento_contable.tipo,

	comprobante_contable.id as id_comprobante_contable,
	prestamo.id as id_prestamo,
	cuenta_contable.id as id_cuenta_contable,
	cuenta_contable_presupuesto.id as id_cuenta_contable_presupuesto,
	cliente.id as id_cliente

from	cuenta_contable_presupuesto right join cuenta_contable 
        on cuenta_contable.id = cuenta_contable_presupuesto.cuenta_contable_id, 
	asiento_contable, comprobante_contable, prestamo, solicitud, cliente

where	comprobante_contable.prestamo_id = prestamo.id
and	asiento_contable.cuenta_contable_id = cuenta_contable.id
and	asiento_contable.comprobante_contable_id = comprobante_contable.id
and	comprobante_contable.prestamo_id = prestamo.id
and	prestamo.solicitud_id = solicitud.id
and	solicitud.cliente_id = cliente.id
and	cliente.id = $P{p_id_cliente}
and	comprobante_contable.fecha_registro = $P{p_fecha}
order by comprobante_contable.id, asiento_contable.id]]></queryString>

