<div class="lista-form" id="form_new" style="display: none"></div>
<%= error %>
<%= message %>
<table class="lista" style="width: 70%">
		<thead>
		<tr>
			<th style="width: 30%" style="text-align: center">
				Nombre del Aspecto
 			</th>
 		<% if @solicitud. Estatus.const_id == "ST0003" %>
			<th style="width: 5%" style="text-align: center"></th>
                <% end %>
		</tr>		
		</thead>
		<tbody id="list_body">
<%  
	even_odd = 1
	for item in @list
		even_odd = 1 - even_odd %>
	<%= render :partial => 'row_edit', :locals => { :item => item, :even_odd => even_odd } -%>
<% end %>
</tbody>
</table>
<br></br>
<%= link_to( image_tag(I18n.t('Sistema.Body.Imagenes.cerrar')),
  { :action => :index },
  { :id=>'button_close' } ) -%>