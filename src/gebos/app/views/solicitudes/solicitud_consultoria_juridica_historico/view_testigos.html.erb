      <%=I18n.t('Sistema.Body.Vistas.General.cedula')%>: <%= text_field 'testigos', 'cedula3', :class=>'text', :maxlength=>'10', :style=>'width: 100px'  -%> <br></br>
    </td>
  </tr>
</table>
<%= image_submit_tag(I18n.t('Sistema.Body.Imagenes.imprimir')) -%>
<%= link_to image_tag(I18n.t('Sistema.Body.Imagenes.imprimir'),:title=>I18n.t('Sistema.Body.Vistas.SolicitudConsultoriaJuridica.Mensajes.imprimir_nota')),
            { :action => "generar_NA"} -%>

<% end %>
<div align ="center" > 
	  <table border="1" bordercolor="gray">
	    <tr>
		<td align ="center">
		  <span style="color: blue"><strong><%=I18n.t('Sistema.Body.Vistas.SolicitudConsultoriaJuridica.Mensajes.recordar_papel')%></strong></span>
                </td>
            </tr>
          </table
