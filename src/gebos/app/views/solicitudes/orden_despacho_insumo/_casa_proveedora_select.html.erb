 <% casa_proveedora_onchange = remote_function(
               :loading =>'image_load()',
               :loaded => 'image_unload()',
               :method=>:get,
               :url=>{:controller=>'orden_despacho_insumo',
               :action=>:casa_proveedora_change },
               :with=>"'id='+value+'&tipo_p=#{tipo}'") %>

      <%= select(
      'casa_proveedora_id', '',
      @casa_proveedora_select.map{|i| [i.nombre, i.id]},
      { :prompt=> I18n.t('Sistema.Body.Vistas.Form.seleccionar') },
      { :class=>'select',:onchange=>casa_proveedora_onchange + ';inputChange();' } ) %>
