<% rubro_onchange = remote_function(
    :loading =>'image_load()',
    :loaded => 'image_unload()',
    :method=>:get,
    :url=>{:controller=>'comite_estadal',
    :action=>:rubro_search},
    :with=>"'id='+value")
%>
<%=
  select("subsector_id", "",
    @subsector.map {|u| [u.nombre,u.id]},
    {:prompt => I18n.t('Sistema.Body.Vistas.Form.seleccionar') },
    { :class=>'select', :onchange=>rubro_onchange+';selectSubsector(this)'})
%> 
