<tr id="row_<%= @solicitud_testigos.id %>_edit">
	<td class="lista-form" colspan="4">
	
		<%= form_title_edit @solicitud_testigos.nombre %>
	
		<%= error "row_#{@solicitud_testigos.id}" %>
		
		<%= form_remote_tag(
		  :loading =>'image_load()',
			:loaded => 'image_unload()',
		  :url => { :action => :save_edit, :id => @solicitud_testigos.id } ) do-%>
	
					<%= render :partial => 'form' %>
					<%= link_to_remote(
                        image_tag(I18n.t('Sistema.Body.Imagenes.cancelar')),
                        :loading =>'image_load()',
						:loaded => 'image_unload()',
                        :method=>:get,
		        :url => { :action => :cancel_edit, :id => @solicitud_testigos.id}) -%>
			   </td>
			<td></td>
			
		    </tr>
		  </table>
		<% end -%>
	</td>
</tr>
