<% municipio_onchange = remote_function(
	:loading =>'image_load()',
	:loaded => 'image_unload()',
    :method=>:get,
	:url=>{:controller=>'envio_maquinaria',
	:action=>:municipio_change },
	:with=>"'municipio_id='+value") %>
<%= select(
  'municipio_id', '',
  @municipio_list.map{|e| [e.nombre, e.id]},
  { :prompt => I18n.t('Sistema.Body.Vistas.Form.seleccionar')},
  { :class=>'select', :onchange=>municipio_onchange + ';inputChange();' } ) %>
