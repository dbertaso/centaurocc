<tr id="row_<%= @cuenta_contable_auxiliar.id %>_edit">
	<td class="lista-form" colspan="6">
	
		<%= form_title_edit @cuenta_contable_auxiliar.codigo + ' - ' + @cuenta_contable_auxiliar.nombre %>

		<%= error "row_#{@cuenta_contable_auxiliar.id}" %>
	
		<%= form_remote_tag(
		  :loading =>'image_load()',
			:loaded => 'image_unload()',
		  :url => { :action => :save_edit, :cuenta_contable_id => @cuenta_contable.id, :id => @cuenta_contable_auxiliar.id } ) -%>
	
			<%= render :partial => 'form' %>
	
			<%= link_to_remote(
	       image_tag(I18n.t('Sistema.Body.Imagenes.cancelar')),
			  :loading =>'image_load()',
				:loaded => 'image_unload()',
	       :url => { :action => :cancel_edit, :id => @cuenta_contable_auxiliar.id}) -%>
				</td>
	    </tr>
	  </table>
	<%= end_form_tag -%>
	</td>
</tr>