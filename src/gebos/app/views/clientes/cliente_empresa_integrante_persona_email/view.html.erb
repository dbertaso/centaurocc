<script type="text/javascript">

  function cerrar()
  {
    window.close();
  }

</script>
<% @view = true %>
<div class="marco-titulo"><div><div>
      <%= I18n.t('Sistema.Body.Vistas.General.consultando') %> <%= "#{I18n.t('Sistema.Body.General.beneficiario')} #{I18n.t('Sistema.Body.Vistas.General.juridico')}" %> <span class="titulo_modificar">'<%= @empresa.alias %>'</span>
    </div></div></div>

<%= render :partial=>'clientes/cliente_empresa/common/tabs' %>

<div class="marco-titulo"><div><div>
      <%= "#{I18n.t('Sistema.Body.Vistas.General.consultando')} #{I18n.t('Sistema.Body.General.integrante')}" %> <span class="titulo_modificar">'<%= @persona.nombre_corto %>'</span>
    </div></div></div>

<%= render :partial=>'clientes/cliente_empresa_integrante_persona/common/tabs' %>
<div class="marco-contenido2">

  <div id="list">
    <%= render :partial=>"list_view"  %>
  </div>
  <% unless params[:popup] == 'true' %>
    <%= link_to( image_tag(I18n.t('Sistema.Body.Imagenes.cerrar')),
      { :controller => 'cliente_empresa_integrante_persona', :action => :view, :popup=>params[:popup], :empresa_id => @empresa.id },
      { :id=>'button_close' } ) -%>
  <% else %>
    <%= image_tag(I18n.t('Sistema.Body.Imagenes.cerrar'),:title=>'Agregar', :onclick=>"javascript:cerrar();", :OnMouseOver=>"style.cursor='pointer'" ) %>
  <% end %>

</div>