<% empresa_telefono = @empresa_telefono if empresa_telefono.nil? %>
<td>
  <%= empresa_telefono.tipo_nombre  %>
</td>
<td>
  <%= empresa_telefono.codigo %>
</td>
<td>
  <%= empresa_telefono.numero %>
</td>
<td align="center">
  <%= format_bool empresa_telefono.fax %>
</td>
<td style="text-align: right">
  <%= link_to_remote(
    image_tag(I18n.t('Sistema.Body.Imagenes.modificar'),:title =>'Editar'),
    :loading =>'image_load()',
    :loaded => 'image_unload()',
    :method =>:get,
    :url => {:action => :edit, :empresa_id => @empresa.id, :id => empresa_telefono.id}) -%>
  <%= link_to_remote(
    image_tag(I18n.t('Sistema.Body.Imagenes.eliminar'),:title =>'Eliminar'),
    :loading =>'image_load()',
    :loaded => 'image_unload()',
    :confirm => I18n.t('Sistema.Body.Vistas.Helpers.Mensajes.esta_seguro'),
    :url => {:action => :delete, :empresa_id => @empresa.id, :id => empresa_telefono.id}) -%>
</td>