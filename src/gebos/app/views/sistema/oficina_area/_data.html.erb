<% items = @items if items.nil? %>
<td>
  <%= items.estado.nombre  %>
</td>
<td>
  <%= items.municipio.nombre %>
</td>
<td>
  <%= items.parroquia.nombre %>
</td>
<td style="text-align: right">
  <%= link_to(image_tag(I18n.t('Sistema.Body.Imagenes.modificar'), :title=>I18n.t('Sistema.Body.Vistas.General.modificar')),
    :action => 'edit', :method => :get, :oficina_id => @oficina.id, :id => items.id, :popup=>params[:popup]) %>&nbsp;
  <%= link_to_remote(image_tag(I18n.t('Sistema.Body.Imagenes.eliminar'), :title=>I18n.t('Sistema.Body.Controladores.Accion.eliminar')),
    :loading =>'image_load()',
    :loaded => 'image_unload()',
    :confirm => I18n.t('Sistema.Body.Vistas.Helpers.Mensajes.esta_seguro'),
    :url => {:action => :delete,  :oficina_id => @oficina.id, :id => items.id}) -%>
</td>