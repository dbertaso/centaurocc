    <%= form_title_edit @calidad_agua_alimento.alimento_disponible %>

    <%= error "row_#{@calidad_agua_alimento.id}" %>

    <%= form_remote_tag(
      :loading =>'image_load()', :loaded => 'image_unload()',
      :url => { :action => :save_edit, :id => @visitas, :id_calidad_agua_alimento => @calidad_agua_alimento.id }) do -%>
