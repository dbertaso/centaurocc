<tr id="row_<%= @embarcacion.id %>_view">
  <td class="lista-form" colspan="8">

    <%= form_title_view @embarcacion.nombre_embarcacion %>

    <%= error "row_#{@embarcacion.id}" %>

    <%= form_remote_tag(
      :loading =>'image_load()',
      :loaded => 'image_unload()',
      :url => { :action => :view, :id => @visita,:id_embarcacion=>@embarcacion.id } ) do -%>

      <%= render :partial => 'form_view' %>
      <%= link_to_remote(image_tag(image_path(I18n.t('Sistema.Body.Imagenes.cancelar')), :title=>I18n.t('Sistema.Body.Vistas.General.cancelar')),
        :method=>:get, :loading =>'image_load()', :loaded => 'image_unload()', :url => { :action => :cancel_view, :id => @visita,:id_embarcacion=>@embarcacion.id}) -%>
    </td>
  </tr>
  </table>
<% end -%>
</td>
</tr>
