        </div>
      </li> 
    <%
    end
    if controller_name=='arte_pesca'
    %>
      <li id="actual"><div><a href="#"><%=I18n.t('Sistema.Body.Controladores.ArtePesca.FormTitles.form_title_records')%></a></div></li>
    <%
    else
    %>
      <li><div>
          <%= link_to(I18n.t('Sistema.Body.Controladores.ArtePesca.FormTitles.form_title_records'), :controller => '/visitas/control_visitas_pesca/arte_pesca', :action => 'index', :id=>params[:id],:siniestro_checked=>params[:siniestro_checked], :popup=>params[:popup], :id_embarcacion => @id_embarcacion) if (action_name == 'index' || action_name == 'edit' || action_name == 'new' || action_name == 'vinculo') %>
