        </div>
      </li>
    <%
    end
    %>
    <%
    if controller_name=='visita_decision'
    %>
      <li id="actual"><div><a href="#"><%=I18n.t('Sistema.Body.Vistas.General.recomendacion')%></a></div></li>
    <%
    else
    %>
      <li><div>
          <%= link_to(I18n.t('Sistema.Body.Vistas.General.recomendacion'), :controller => '/visitas/visita_decision', :action => 'edit', :id => @visitas.id,:siniestro_checked=>params[:siniestro_checked], :popup=>params[:popup]) if (action_name == 'index' || action_name == 'edit') %>

        </div>
      </li>
    <%
    end
  %>
    <% if controller_name=='siniestro' %>
      <li id="actual"><div><a href="#">Pérdida</a></div></li> 
    <%elsif params[:siniestro_checked] == "1"%>
      <li>
        <div>
          <%= link_to('Pérdida', :controller => '/solicitudes/siniestro', :action => 'index', :visita_id => @visitas.id,:id=>params[:id],:siniestro_checked=>1, :popup=>params[:popup])  %>
        </div>
      </li>
    <%end%>
    <%if @visitas.decision_visita
      unless !@visitas.decision_visita.recomienda_generar_desembolso
        if controller_name=='recomendacion_plan_inversion'
        %>
          <li id="actual"><div><a href="#"><%=I18n.t('Sistema.Body.Vistas.VisitaSeguimientoCultivo.Etiquetas.recomendacion_desembolso')%></a></div></li>
        <%
        else
        %>
          <li><div>
              <%= link_to(I18n.t('Sistema.Body.Vistas.VisitaSeguimientoCultivo.Etiquetas.recomendacion_desembolso'), :controller => '/visitas/recomendacion_plan_inversion', :action => 'index', :id => @visitas.id,:siniestro_checked=>params[:siniestro_checked], :popup=>params[:popup])  %>
