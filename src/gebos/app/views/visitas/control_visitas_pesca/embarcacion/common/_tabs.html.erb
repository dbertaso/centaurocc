<div id="tabs">
  <ul>
    <%
    action_name = controller.action_name
    controller_name = controller.controller_name
    if controller_name=='embarcacion'
    %>
      <li id="actual"><div><a href="#"><%=I18n.t('Sistema.Body.Vistas.General.embarcacion')%></a></div></li>
    <%
    else
    %>
      <li><div>
          <%= link_to(I18n.t('Sistema.Body.Vistas.General.embarcacion'), :controller => '/visitas/control_visitas_pesca/embarcacion', :action => 'index', :id=>params[:id],:siniestro_checked=>params[:siniestro_checked],:popup=>params[:popup]) if (action_name == 'index' || action_name == 'edit' || action_name == 'new' || action_name == 'vinculo') %>
          <%= link_to(I18n.t('Sistema.Body.Vistas.General.embarcacion'), :controller => '/visitas/control_visitas_pesca/embarcacion', :action => 'view', :id=>params[:id],:siniestro_checked=>params[:siniestro_checked],:popup=>params[:popup]) if (action_name=='view' || action_name=='view_detail' || action_name == 'vinculo_view') %>
        </div>
      </li>
    <%
    end
    if controller_name=='motores'
    %>
      <li id="actual"><div><a href="#"><%=I18n.t('Sistema.Body.Vistas.General.motores')%></a></div></li> 
    <%
    else
    %>
      <li><div>
          <%= link_to(I18n.t('Sistema.Body.Vistas.General.motores'), :controller => '/visitas/control_visitas_pesca/motores', :action => 'index', :id=>params[:id],:siniestro_checked=>params[:siniestro_checked], :popup=>params[:popup], :id_embarcacion => @id_embarcacion) if (action_name == 'index' || action_name == 'edit' || action_name == 'new' || action_name == 'vinculo') %>
