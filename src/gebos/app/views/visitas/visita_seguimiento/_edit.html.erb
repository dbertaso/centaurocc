<tr id="row_<%= @seguimiento_visita.id %>_edit">
  <td class="lista-form" colspan="5">

    <%= form_title_edit @seguimiento_visita.codigo_visita %>

    <%= error "row_#{@seguimiento_visita.id}" %>

    <%= form_remote_tag(
      :loading =>'image_load()',
      :loaded => 'image_unload()',
      :url => { :action => :save_edit, :id => @seguimiento_visita.id } ) do %>
      
  <%= render :partial => 'form' %>

      <%= link_to_remote(image_tag(image_path(I18n.t('Sistema.Body.Imagenes.cancelar')), :title=>I18n.t('Sistema.Body.Vistas.General.cancelar')),
        :method=>:get, :loading =>'image_load()', :loaded => 'image_unload()',
        :url => { :action => :cancel_edit, :id => @seguimiento_visita.id}) -%>
    </td>
  </tr>
  </table>
<% end %>
</td>
</tr>
