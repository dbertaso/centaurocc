<tr id="row_<%= @programa_tipo_gasto.id %>_edit">
<script language="JavaScript">
  varEnviado= false;
</script>
<td class="lista-form" colspan="5">
  <%= I18n.t('Sistema.Body.Vistas.ProgramaGasto.Header.form_title_modificar')%> <span class="titulo_modificar">'<%= @programa.alias %>'</span>

  <%= error "row_#{@programa_tipo_gasto.id}" %>


  <%= form_remote_tag(
    :loading =>'image_load()', :loaded => 'image_unload()',
    :url => { :action => :save_edit, :id=>@programa_tipo_gasto.id, :programa_id => @programa.id } ) do -%>
    <%= render :partial => 'form' %>
    <%= link_to_remote(image_tag(image_path(I18n.t('Sistema.Body.Imagenes.cancelar'))),
      :loading =>'image_load()', :loaded => 'image_unload()',
      :url => { :action => :cancel_edit, :id=>@programa_tipo_gasto.id, :programa_id => @programa.id}) -%>
  </td>
  </tr>
  </table>
<% end -%>
</td>

</tr>
