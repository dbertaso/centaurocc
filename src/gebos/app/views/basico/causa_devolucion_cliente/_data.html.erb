<% causa_devolucion_cliente = @causa_devolucion_cliente if causa_devolucion_cliente.nil? %>
<td>
	<%= causa_devolucion_cliente.nombre  %>
</td>
<td>
	<%= causa_devolucion_cliente.descripcion  %>
</td>	
<td style="text-align: center">
	<%= format_bool(causa_devolucion_cliente.activo)  %>
</td>	
<td style="text-align: right" NOWRAP>
		
	<%= link_to_remote(
		image_tag(I18n.t('Sistema.Body.Imagenes.modificar')),
	  :loading =>'image_load()',
 	  :loaded => 'image_unload()',
    :method =>:get,
		:url => {:controller => 'causa_devolucion_cliente', :action => :edit, :id => causa_devolucion_cliente.id} ) if vlink 'edit' -%>
	<%= link_to_remote(
		image_tag(I18n.t('Sistema.Body.Imagenes.eliminar')),
	  :loading =>'image_load()',
 	  :loaded => 'image_unload()',
	:confirm => I18n.t('Sistema.Body.Vistas.Helpers.Mensajes.esta_seguro'),
		:url => {:controller => 'causa_devolucion_cliente', :action => :delete, :id => causa_devolucion_cliente.id}) if vlink 'delete' -%>
	
</td>
