<div class="marco-titulo">
  <div>
    <div>
      <%= I18n.t('Sistema.Body.Vistas.ProgramaProducto.Header.form_title_modificar')%> <span class="titulo_modificar">'<%= @programa.alias %>'</span>
    </div>
  </div>
</div>

<%= render 'basico/programa/common/tabs' %>
<div class="marco-contenido2">

  <div class="marco-boton-in" id="button_new">
    <%= link_to(image_tag(image_path(I18n.t('Sistema.Body.Imagenes.agregar'))),
    :action => 'new', :programa_id => @programa.id ) if vlink 'edit' %>
  </div>

  <%= error %>
  <%= message %>

  <div id="list">
    <%= render "list" %>
  </div>

  <%= link_to( image_tag(image_path(I18n.t('Sistema.Body.Imagenes.cerrar'))),
    { :controller => 'programa', :action => :index },
    { :id=>'button_close' } ) -%>

</div>
