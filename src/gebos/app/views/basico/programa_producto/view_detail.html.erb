<div class="marco-titulo">
  <div>
    <div>
      <%= I18n.t('Sistema.Body.Vistas.ProgramaProducto.Header.form_title_consultar')%>  <span class="titulo_modificar">'<%= @programa.alias %>'</span>
    </div>
  </div>
</div>

<%= render 'basico/programa/common/tabs' %>
<div class="marco-contenido2">

   <%= form_title_view @producto.sector.nombre %>

  <%= render 'basico/programa_producto/common/tabs' %>
  <div class="marco-contenido2">
    <%= error  %>
    <%= message  %>


    <%= render 'form_view' %>

      <div id="buttons_edit" style="display: none">
        <%= form_buttons_edit :programa_id => @programa.id %>
      </div>

      <%= link_to( image_tag(I18n.t('Sistema.Body.Imagenes.cerrar')),
        { :action => :view, :programa_id => @programa.id },
        { :id=>'button_close', } ) -%>

    <%= end_form_tag -%>
