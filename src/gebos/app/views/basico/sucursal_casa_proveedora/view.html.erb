<%= form_title_view @sucursal_casa_proveedora.nombre %>

<div class="marco-contenido2">

	<table>
		<tr>
			<td style="width: 100px">
				<label><%=I18n.t('Sistema.Body.Vistas.General.region')%></label>
			</td>
			<td style="width: 600px">
				<%= @sucursal_casa_proveedora.region.nombre -%>
			</td>
		</tr>
		<tr>
			<td>
				<label><%=I18n.t('Sistema.Body.Vistas.General.nombre') << " " << I18n.t('Sistema.Body.Vistas.General.de') << " " << I18n.t('Sistema.Body.Vistas.General.la') << " " << I18n.t('Sistema.Body.Vistas.General.sucursal')%></label>
			</td>
			<td>
				<%= @sucursal_casa_proveedora.nombre -%>
			</td>
		</tr>
	</table>

	<%= link_to( image_tag(I18n.t('Sistema.Body.Imagenes.boton_modificar')),
		:action => :edit,
		:id => @sucursal_casa_proveedora.id,
		:html_options => { :id=>'button_edit' } ) if vlink 'edit' -%>


	<%= link_to( image_tag(I18n.t('Sistema.Body.Imagenes.cerrar')),
		{ :action => :index },
		{ :id=>'button_close' } ) -%>

</div>

