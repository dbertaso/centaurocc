<tr id="row_<%= @comite.id %>_edit">
	<td class="lista-form" colspan="7">
	
		<%= form_title_edit @comite.numero %>
	
		<%= error "row_#{@comite.id}" %>
	
		<%= form_remote_tag(
		  :loading =>'image_load()',
			:loaded => 'image_unload()',
		  :url => { :action => :save_edit, :id => @comite.id, :accion =>'new' } ) do-%>
					<%= render :partial => 'form_edit' %>
					<%= link_to_remote(
		        image_tag(I18n.t('Sistema.Body.Imagenes.cancelar')),
					  :loading =>'image_load()',
					  :loaded => 'image_unload()',
                      :method=>:get,
		        :url => { :action => :cancel_edit, :id => @comite.id}) -%>
			   </td>
		    </tr>
		  </table>
		<% end -%>
	</td>
</tr>
