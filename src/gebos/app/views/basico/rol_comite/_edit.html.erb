<tr id="row_<%= @rol_comite.id %>_edit">
  <td class="lista-form" colspan="4">

    <%= form_title_edit @rol_comite.nombre %>

    <%= error "row_#{@rol_comite.id}" %>

    <%= form_remote_tag(
      :loading =>'image_load()',
      :loaded => 'image_unload()',
      :url => { :action => :save_edit, :id => @rol_comite.id } ) do -%>

    <%= render :partial => 'form' %>
    <%= link_to_remote(
      image_tag(I18n.t('Sistema.Body.Imagenes.cancelar')),
      :loading =>'image_load()',
      :loaded => 'image_unload()',
      :method =>:get,
      :url => { :action => :cancel_edit, :id => @rol_comite.id}) -%>
  </td>
  <td></td>
  <td></td>
  <td></td>
</tr>
</table>
<% end -%>
</td>
</tr>
