<tr id="row_<%= @proveedor_marino.id %>_edit">
  <td class="lista-form" colspan="5">

    <%= form_title_edit @proveedor_marino.nombre %>

    <%= error "row_#{@proveedor_marino.id}" %>

    <%= form_remote_tag(
      :loading =>'image_load()',
      :loaded => 'image_unload()',
      :url => { :action => :save_edit, :id => @proveedor_marino.id } ) do -%>
      <%= render :partial => 'form' %>
      <%= link_to_remote(
        image_tag(I18n.t('Sistema.Body.Imagenes.cancelar')),
        :loading =>'image_load()',
        :loaded => 'image_unload()',
        :method =>:get,
        :url => { :action => :cancel_edit, :id => @proveedor_marino.id}) -%>
    </td>
  </tr>
  </table>
  <% end -%>
  </td>
  </tr>
