

<div class="marco-titulo">
  <div>
    <div>
      <%= I18n.t('Sistema.Body.Vistas.ItemAtributo.Header.form_title')%> 
      <span class="titulo_modificar">'<%= @item.nombre %>'</span>
    </div>
  </div>
</div>
<%= render 'basico/sector/common/tabs' %>

<div class="marco-contenido2">
  <%= render '/basico/item/common/tabs' %>

  <div class="marco-contenido2">
    <div class="marco-boton-in" id="button_new">
    <%= link_to(image_tag(image_path(I18n.t('Sistema.Body.Imagenes.agregar'))),
      :action => 'new', :item_id => @item.id, :partida_id => @partida.id, :popup=>params[:popup] ) if vlink 'edit' %>
    </div>

    <%= error %>
    <%= message %>

    <div class="lista-form" id="form_new" style="display: none"></div>

    <div id="list">
      <%= render 'list' %>
    </div>

    <%= link_to(image_tag(image_path(I18n.t('Sistema.Body.Imagenes.cerrar'))),
      { :controller=> 'sector', :action => :index},
      { :id=>'button_close' } ) -%>

  </div>
</div>

