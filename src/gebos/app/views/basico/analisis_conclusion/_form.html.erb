<script language="javascript">varEnviado = false;</script>
<table style="width: 100%">

  <tr>
    <td style="width: 110px">
      <label><%= I18n.t('Sistema.Body.Vistas.General.numero') %></label> <label class="requerido"> (*) </label>
    </td>
    <td style="width: 620px">
      <%= text_field 'analisis_conclusion', 'numero', :class=>'text', :style=>'width: 500px', :maxlength=>'10'  -%>
    </td>
  </tr>

  <tr>
    <td style="width: 150px">
      <label><%= I18n.t('Sistema.Body.Vistas.General.nombre') %></label> <label class="requerido"> (*) </label>
    </td>
    <td style="width: 580px">
      <%= text_field 'analisis_conclusion', 'nombre', :class=>'text', :style=>'width: 500px', :maxlength=>'200'  -%>
    </td>
  </tr>

  <tr>
    <td>
      <label><%=  I18n.t('Sistema.Body.Vistas.General.activo')%></label>
    </td>
    <td>
      <%= check_box 'analisis_conclusion', 'activo',
        :checked_value=>"1",
        :unchecked_value=>"0",
        :onclick=>'inputChange();' -%>
    </td>
  </tr>

  <tr>
    <td colspan="2" style="text-align: left">
      <%= image_submit_tag(I18n.t('Sistema.Body.Imagenes.guardar'), :title=> I18n.t('Sistema.Body.Vistas.General.guardar'), :onclick=>"if (varEnviado) { return false; } else {varEnviado = true;}", :id=>'button_save') -%>

