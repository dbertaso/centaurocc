<% item = @analisis_conclusion if item.nil? %>
<td style="text-align: center">
  <%= item.numero %>
</td>
<td style="text-align: center">
  <%= item.nombre %>
</td>
<td style="text-align: center">
  <%= format_bool(item.activo)  %>
</td>


<td style="text-align: center">
  <%= link_to_remote(
    image_tag(I18n.t('Sistema.Body.Imagenes.modificar'), :title=> I18n.t('Sistema.Body.Vistas.General.modificar')),
    :method => :get,
    :loading =>'image_load()',
    :loaded => 'image_unload()',
    :url => {:controller => 'analisis_conclusion', :action => :edit, :id => item.id}) if vlink 'edit' -%>
  <%= link_to_remote(image_tag(I18n.t('Sistema.Body.Imagenes.eliminar'), :title=> I18n.t('Sistema.Body.Controladores.Accion.eliminar')),
    :loading =>'image_load()',
    :loaded => 'image_unload()',
    :confirm => I18n.t('Sistema.Body.Vistas.Helpers.Mensajes.esta_seguro'),
    :url => {:controller => 'analisis_conclusion', :action => :delete, :id => item.id}) if vlink 'delete' -%>
</td>
