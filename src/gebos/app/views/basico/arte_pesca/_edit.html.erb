<tr id="row_<%= @arte_pesca.id %>_edit">
  <td class="lista-form" colspan="8">

    <%= form_title_edit @arte_pesca.nombre_arte_pesca %>

    <%= error "row_#{@arte_pesca.id}" %>

    <%= form_remote_tag(
      :loading =>'image_load()',
      :loaded => 'image_unload()',
      :url => { :action => :save_edit, :id_pesca_index=>@arte_pesca.id,:id=>params[:id] } ) do -%>      
    <%= render :partial => 'form' %>        
    <%= link_to_remote(image_tag(I18n.t('Sistema.Body.Imagenes.cancelar')),
      :loading =>'image_load()',
      :loaded => 'image_unload()',
      :method =>:get,
      :url => { :action => :cancel_edit, :id => @arte_pesca.id}) -%>
  </td>
</tr>
</table>
<% end -%>
</td>
</tr>
