<% parroquia_onchange = remote_function(
    :loading =>'image_load()',
    :loaded => 'image_unload()',
    :method => :get,
    :url=>{:controller=>'casa_proveedora',
    :action=>:parroquia_search },
    :with=>"'id='+value")
%>			
<%=
  select("casa_proveedora", "municipio_id",
    @municipio.map {|u| [u.nombre,u.id]},
    {:prompt => I18n.t('Sistema.Body.Vistas.Form.seleccionar')},
    { :class=>'select', :onchange=>parroquia_onchange+';inputChange();'})
%>
