<tr id="row_<%= @parroquia.id %>_edit">
	<td class="lista-form" colspan="2">
	
		<%= form_title_edit @parroquia.nombre %>
	
		<%= error "row_#{@parroquia.id}" %>
		
		<%= form_remote_tag(
		  :loading =>'image_load()',
			:loaded => 'image_unload()',
		  :url => { :action => :save_edit, :municipio_id => params[:municipio_id], :id => @parroquia.id } ) do-%>
	
					<%= render 'form' %>
					<%= link_to_remote(image_tag(I18n.t('Sistema.Body.Imagenes.cancelar')),
					  :loading =>'image_load()',
					  :loaded => 'image_unload()',
					  :method=>:get,
		        :url => { :action => :cancel_edit, :municipio_id => params[:municipio_id], :id => @parroquia.id}) -%>
			   </td>
		    </tr>
		  </table>
		<% end -%>
	</td>
</tr>