<tr id="row_<%= @tipo_arte_pesca.id %>_edit">
	<td class="lista-form" colspan="5">
	
		<%= form_title_edit @tipo_arte_pesca.tipo %>
	
		<%= error "row_#{@tipo_arte_pesca.id}" %>
		
		<%= form_remote_tag(
		  :loading =>'image_load()',
			:loaded => 'image_unload()',
		  :url => { :action => :save_edit, :id => @tipo_arte_pesca.id } ) do-%>
	
					<%= render 'form' %>
					<%= link_to_remote(image_tag(I18n.t('Sistema.Body.Imagenes.cancelar')),
					:method=>:get,
					  :loading =>'image_load()',
						:loaded => 'image_unload()',
		        :url => { :action => :cancel_edit, :id => @tipo_arte_pesca.id}) -%>
			   </td>
			
		    </tr>
		  </table>
		<% end -%>
	</td>
</tr>
