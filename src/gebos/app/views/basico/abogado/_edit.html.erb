<tr id="row_<%= @abogado.id %>_edit">
	<td class="lista-form" colspan="7">
	
		<%= form_title_edit @abogado.nombre %>
	
		<%= error "row_#{@abogado.id}" %>
		
		<%= form_remote_tag(
		  :loading =>'image_load()',
			:loaded => 'image_unload()',
		  :url => { :action => :save_edit, :id => @abogado.id }) do -%>
	
					<%= render 'form' %>
					<%= link_to_remote(image_tag(I18n.t('Sistema.Body.Imagenes.cancelar')),
                       :method => :get,
					  :loading =>'image_load()',
						:loaded => 'image_unload()',
		        :url => { :action => :cancel_edit, :id => @abogado.id}) -%>
			   </td>			
		    </tr>
		  </table>
		<% end -%>
	</td>
</tr>
