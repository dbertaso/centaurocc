

<tr id="row_<%= @cronograma_desembolso.id %>_edit">
  <td class="lista-form" colspan="8">

    <%#= form_title_edit @cronograma_desembolso.numero_desembolso %>

    <%= error "row_#{@cronograma_desembolso.id}" %>

    <%= form_remote_tag(
      :loading =>'image_load()',
      :loaded => 'image_unload()',
      :url => { :action => :save_edit, :id => @cronograma_desembolso.id } )  do-%>
          <%= render 'form' %>
          <%= link_to_remote(
            image_tag(I18n.t('Sistema.Body.Imagenes.cancelar')),
            :loading =>'image_load()',
            :loaded => 'image_unload()',
            :method => :get,
            :url => { :action => :cancel_edit, :id => @cronograma_desembolso.id}) -%>
         </td>
        </tr>
      </table>
    <%end-%>
  </td>
</tr>


