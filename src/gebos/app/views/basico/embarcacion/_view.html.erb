<tr id="row_<%= @embarcacion.id %>_view">
  <td class="lista-form" colspan="8">

    <%= form_title_view @embarcacion.nombre_embarcacion %>

    <%= error "row_#{@embarcacion.id}" %>

    <%= render :partial => 'form_view' %>
    <%= link_to_remote(
      image_tag(I18n.t('Sistema.Body.Imagenes.cancelar')),
      :loading =>'image_load()',
      :loaded => 'image_unload()',
      :url => { :action => :cancel_view, :id => @embarcacion.id}) -%>
  </td>
</tr>
</table>
</td>
</tr>