
<%= select(
  'sub_sector_id', '',
  @sub_sector_list.map{|e| [e.nombre, e.id]},
     {:prompt=>'Seleccionar '},
    { :class=>'select', :onchange=>"#{remote_function(:loading =>'Element.show("loading")',
    :loaded => 'Element.hide("loading")',
    :method => :get, :url=>{:controller=>'detalle_precio_gaceta',
      :action=>:sub_sector_change }, :with => "'sub_sector_id=' + this.value")};inputChange();" } )%>
