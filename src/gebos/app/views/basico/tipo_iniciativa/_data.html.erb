<% tipo_iniciativa = @tipo_iniciativa if tipo_iniciativa.nil? %>
<td style="text-align: center">
  <%= tipo_iniciativa.nombre %>
</td>
<td style="text-align: center">
  <%= link_to_remote(
    image_tag(I18n.t('Sistema.Body.Imagenes.modificar'), :title=>I18n.t('Sistema.Body.Vistas.General.modificar')),
    :method => :get,
    :loading =>'image_load()',
    :loaded => 'image_unload()',
    :url => {:controller => 'tipo_iniciativa', :action => :edit, :id => tipo_iniciativa.id}) if vlink 'edit' -%>&nbsp;
  <%= link_to_remote(image_tag(I18n.t('Sistema.Body.Imagenes.eliminar'), :title=>I18n.t('Sistema.Body.Controladores.Accion.eliminar')),
    :loading =>'image_load()',
    :loaded => 'image_unload()',
    :confirm => I18n.t('Sistema.Body.Vistas.Helpers.Mensajes.esta_seguro'),
    :url => {:controller => 'tipo_iniciativa', :action => :delete, :id => tipo_iniciativa.id}) if vlink 'delete' -%>
</td>