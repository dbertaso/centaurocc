<tr id="row_<%= @empresa_transporte_maquinaria.id %>_edit">
  <td class="lista-form" colspan="9">

    <%= form_title_edit @empresa_transporte_maquinaria.nombre %>

    <%= error "row_#{@empresa_transporte_maquinaria.id}" %>

    <%= form_remote_tag(
      :loading =>'image_load()',
      :loaded => 'image_unload()',
      :url => { :action => :save_edit, :id => @empresa_transporte_maquinaria.id } ) do-%>
    <%= render 'form' %>
    <%= link_to_remote(
      image_tag(I18n.t('Sistema.Body.Imagenes.cancelar')),
      :loading =>'image_load()',
      :loaded => 'image_unload()',
      :method=>:get,
      :url => { :action => :cancel_edit, :id => @empresa_transporte_maquinaria.id}) -%>
  </td>
</tr>
</table>
<% end -%>
</td>
</tr>
