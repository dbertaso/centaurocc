<tr id="row_<%= @garantia_sector.id %>_edit">
  <td class="lista-form" colspan="5">

    <%= form_title_edit @garantia_sector.sub_sector.sector.nombre.to_s %>

    <%= error "row_#{@garantia_sector.id}" %>

    <%= form_remote_tag(
      :loading =>'image_load()',
      :loaded => 'image_unload()',
      :url => { :action => :save_edit, :id => @garantia_sector.id } ) do -%>
    <%= render  'form' %>
    <%= link_to_remote(
      image_tag(image_path(I18n.t('Sistema.Body.Imagenes.cancelar'))),
      :loading =>'image_load()',
      :loaded => 'image_unload()',
      :url => { :action => :cancel_edit, :id => @garantia_sector.id}) -%>
  </td>
</tr>
</table>
<% end -%>
</td>
</tr>
