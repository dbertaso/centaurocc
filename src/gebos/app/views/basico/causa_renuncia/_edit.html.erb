<tr id="row_<%= @causa_renuncia.id %>_edit">
	<td class="lista-form" colspan="4">
	
		<%= form_title_edit @causa_renuncia.nombre %>
	
		<%= error "row_#{@causa_renuncia.id}" %>
	
		<%= form_remote_tag(
		  :loading =>'image_load()',
			:loaded => 'image_unload()',
		  :url => { :action => :save_edit, :id => @causa_renuncia.id } ) do-%>
					<%= render 'form' -%>
					<%= link_to_remote(
		        image_tag(I18n.t('Sistema.Body.Imagenes.cancelar')),
		        :method => :get,
					  :loading =>'image_load()',
						:loaded => 'image_unload()',
		        :url => { :action => :cancel_edit, :id => @causa_renuncia.id}) -%>
			   </td>
		    </tr>
		  </table>
		<% end -%>
	</td>
</tr>
