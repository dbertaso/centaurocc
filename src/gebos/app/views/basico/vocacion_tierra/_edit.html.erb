<tr id="row_<%= @vocacion_tierra.id %>_edit">
	<td class="lista-form" colspan="4">
	
		<%= form_title_edit @vocacion_tierra.nombre %>
	
		<%= error "row_#{@vocacion_tierra.id}" %>
		
		<%= form_remote_tag(
		  :loading =>'image_load()',
			:loaded => 'image_unload()',
		  :url => { :action => :save_edit, :id => @vocacion_tierra.id } ) do -%>
	
					<%= render 'form' %>
					<%= link_to_remote(image_tag(I18n.t('Sistema.Body.Imagenes.cancelar')),
					:method=> :get,
					  :loading =>'image_load()',
						:loaded => 'image_unload()',
		        :url => { :action => :cancel_edit, :id => @vocacion_tierra.id}) -%>
			   </td>
			<td></td>
			<td></td>
			<td></td>
		    </tr>
		  </table>
		<% end -%>
	</td>
</tr>
