<% list = @list if list.nil? %>
<td align="center">
<%
def cas_mes(mes)
mes_casteado = ''
if mes.to_i == 1 
	mes_casteado = 'ENE'
end

if mes.to_i == 2 
	mes_casteado = 'FEB'
end
if mes.to_i == 3
	mes_casteado = 'MAR'
end
if mes.to_i == 4
	mes_casteado = 'ABR'
end
if mes.to_i == 5 
	mes_casteado = 'MAY'
end
if mes.to_i == 6
	mes_casteado = 'JUN'
end
if mes.to_i == 7 
	mes_casteado = 'JUL'
end
if mes.to_i == 8 
	mes_casteado = 'AGO'
end
if mes.to_i == 9
	mes_casteado = 'SEP'
end
if mes.to_i == 10 
	mes_casteado = 'OCT'
end
if mes.to_i == 11
	mes_casteado = 'NOV'
end

if mes.to_i == 12
	mes_casteado = 'DIC'
end

return mes_casteado
end
%>
	<%= I18n.t('date.abbr_month_names')[list.mes.to_i] + '/' + list.ano.to_s  %>
</td>	

<td data-align="right">
	<% unless list.saldo.nil? %>
		<%= format_fm(list.saldo) -%>
	<% else %> 0,00 <% end %> 
</td>	
